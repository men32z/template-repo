<div class="container">
  <div class="row">
    <div class="col-12 mt-5">
      <h4>All Users</h4>
      <table class="table table-striped">
        <thead>
          <th>First Name</th>
          <th>Last Name</th>
        </thead>
        <tbody>
          <%= render @users %>
        </tbody>
      </table>
    </div>
    <div class="col-12">
      <%= paginate @users, params: @params %>
    </div>
  </div>
</div>